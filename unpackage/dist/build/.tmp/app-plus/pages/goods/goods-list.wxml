<view><view class="header" style="{{'position:'+(headerPosition)+';'+('top:'+(headerTop)+';')}}"><block wx:for="{{orderbyList}}" wx:for-item="target" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['select',[index]]]]]}}" class="{{['target',target.selected?'on':'']}}" bindtap="__e">{{''+target.text+''}}<block wx:if="{{target.orderbyicon}}"><view class="{{['icon',target.orderbyicon[target.orderby]]}}"></view></block></view></block></view><view class="place"></view><view class="goods-list"><view class="product-list"><block wx:for="{{goodsList}}" wx:for-item="goods" wx:for-index="__i0__" wx:key="goods_id"><view data-event-opts="{{[['tap',[['toGoods',['$0'],[[['goodsList','goods_id',goods.goods_id]]]]]]]}}" class="product" bindtap="__e"><image src="{{goods.logo}}"></image><view class="name">{{goods.title}}</view><view class="info"><view class="price">{{goods.price}}</view><view class="slogan">{{goods.market_price}}</view></view><view class="evaluate"><view class="evaluate-num">{{goods.comment_num+"条评价"}}</view><view class="evaluate-good">{{goods.good_percent+"%好评"}}</view></view></view></block></view><block wx:if="{{tip}}"><view class="loading-text">{{loadingText}}</view></block></view></view>