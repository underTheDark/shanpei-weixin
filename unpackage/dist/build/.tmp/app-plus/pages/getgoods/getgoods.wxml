<view id="getgoods"><block wx:for="{{productList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['skipWeb',['$0'],[[['productList','',index,'linkurl']]]]]]]}}" class="addr-list" bindtap="__e"><view class="list-left"><view class="photo"><image src="{{item.headimg}}"></image></view><view class="info"><view class="user-info"><view class="name">{{item.name}}</view><view class="star"><uni-rate vue-id="{{'1-'+index}}" value="{{item.star}}" size="18" bind:__l="__l"></uni-rate></view></view><view class="user-addr">{{''+item.address+''}}</view></view></view><view class="list-right"><image src="../../static/img/call.png" data-event-opts="{{[['tap',[['callPhone',['$0'],[[['productList','',index,'phone']]]]]]]}}" bindtap="__e"></image></view><view></view></view></block><uni-load-more vue-id="2" status="{{loading}}" bind:__l="__l"></uni-load-more></view>