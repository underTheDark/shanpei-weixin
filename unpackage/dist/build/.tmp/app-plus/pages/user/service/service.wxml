<view><block wx:for="{{goodsList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="service-list"><view class="service-one">{{"订单编号："+item.order_no}}</view><view class="service-two"><block wx:for="{{item.order_list}}" wx:for-item="row" wx:for-index="num" wx:key="num"><view class="list"><view class="service-two-left"><image src="{{row.goods_logo}}"></image></view><view class="service-two-right"><text>{{''+row.goods_title+''}}</text><view class="price-num"><text class="price">{{row.goods_spec}}</text><text>{{"x"+row.number}}</text></view></view></view></block></view><view class="service-three"><text>退</text><text>{{item.refund_type==1?"退货退款":item.refund_type==2?"换货":""}}</text><text>{{item.refund_state==0?"未退款":item.refund_type==1?"待退款":item.refund_type==2?"已退款":""}}</text></view></view></block></view>